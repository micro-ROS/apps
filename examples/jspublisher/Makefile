-include $(TOPDIR)/Make.defs

# uROS 3D PRO Joystick-publisher built-in application info

CONFIG_UROS_EXAMPLES_JSPUBLISHER_PRIORITY ?= SCHED_PRIORITY_DEFAULT
CONFIG_UROS_EXAMPLES_JSPUBLISHER_STACKSIZE ?= 65000

APPNAME = jspublisher
PRIORITY = $(CONFIG_UROS_EXAMPLES_JSPUBLISHER_PRIORITY)
STACKSIZE = $(CONFIG_UROS_EXAMPLES_JSPUBLISHER_STACKSIZE)

# uROS 3D PRO Joystick-publisher Example

ASRCS =
CSRCS = js_driver.c init_js_6lowpan.c
MAINSRC = jspublisher_main.c

CONFIG_UROS_EXAMPLES_JSPUBLISHER_PROGNAME ?= jspublisher$(EXEEXT)
PROGNAME = $(CONFIG_UROS_EXAMPLES_JSPUBLISHER_PROGNAME)
UROS_INCLUDES = $(shell find $(APPDIR)/$(CONFIG_UROS_DIR)/install -type d -name include)
CFLAGS += ${shell $(INCDIR) $(INCDIROPT) "$(CC)" "$(UROS_INCLUDES)"} -std=c99

MODULE = CONFIG_UROS_EXAMPLES_JSPUBLISHER

include $(APPDIR)/Application.mk
