-include $(TOPDIR)/Make.defs

# USB Host Joystick

CONFIG_EXAMPLES_JOYSTICK_DEFPRIO ?= 100
CONFIG_EXAMPLES_JOYSTICK_STACKSIZE ?= 2048

APPNAME = js
PRIORITY = $(CONFIG_EXAMPLES_JOYSTICK_DEFPRIO)
STACKSIZE = $(CONFIG_EXAMPLES_JOYSTICK_STACKSIZE)

ASRCS =
CSRCS =
MAINSRC = js_main.c

CONFIG_EXAMPLES_JOYSTICK_PROGNAME ?= js$(EXEEXT)
PROGNAME = $(CONFIG_EXAMPLES_JOYSTICK_PROGNAME)

MODULE = CONFIG_EXAMPLES_JOYSTICK

include $(APPDIR)/Application.mk
