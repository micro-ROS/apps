-include $(TOPDIR)/Make.defs

# uROS-publisher-distance built-in application info

CONFIG_UROS_EXAMPLES_DEMO_DISTANCE_PUBLISHER_PRIORITY ?= SCHED_PRIORITY_DEFAULT
CONFIG_UROS_EXAMPLES_DEMO_DISTANCE_PUBLISHER_STACKSIZE ?= 65000

APPNAME = demo_distance
PRIORITY = $(CONFIG_UROS_EXAMPLES_DEMO_DISTANCE_PUBLISHER_PRIORITY)
STACKSIZE = $(CONFIG_UROS_EXAMPLES_DEMO_DISTANCE_PUBLISHER_STACKSIZE)

# uROS-publisher Example

ASRCS =
CSRCS =
MAINSRC = demo_distance.c 

CONFIG_UROS_EXAMPLES_DEMO_DISTANCE_PUBLISHER_PROGNAME ?= demo_distance$(EXEEXT)
PROGNAME = $(CONFIG_UROS_EXAMPLES_DEMO_DISTANCE_PUBLISHER_PROGNAME)

UROSDIR = $(shell find $(APPDIR)/$(CONFIG_UROS_DIR)/install -type d -name include)
CFLAGS += ${shell $(INCDIR) $(INCDIROPT) "$(CC)" "$(UROSDIR)"} -std=c99

MODULE = CONFIG_UROS_EXAMPLES_DEMO_DISTANCE_PUBLISHER

include $(APPDIR)/Application.mk
